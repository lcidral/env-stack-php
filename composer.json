{
  "name": "lcidral/developstack",
  "description": "this repository is a stack of tools for development in php with docker and composer",
  "license": "GLWTPL",
  "authors": [
    {
      "name": "Leonardo Cidral",
      "email": "lcidral@gmail.com"
    }
  ],
  "minimum-stability" : "dev",
  "config" : {
    "bin-dir" : "bin"
  },
  "autoload": {
    "psr-4": {
      "lcidral\\developstack\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2.4",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "robmorgan/phinx": "0.9.2",
    "illuminate/database": "~5.1",
    "ramsey/uuid": "3.7.3",
    "predis/predis": "^2.0@dev",
    "filippo-toso/driver-rocket-chat": "dev-master",
    "botman/botman": "2.0.x-dev",
    "botman/installer": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev" : {
    "phpunit/phpunit": "7.1.1",
    "codeception/codeception": "2.4.x-dev",
    "phpunit/php-code-coverage": "^6.0@dev",
    "captbaritone/mailcatcher-codeception-module": "1.*",
    "codeception/c3": "2.*",
    "mockery/mockery": "dev-master"
  }
}

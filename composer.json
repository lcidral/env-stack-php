{
  "name": "lcidral/env-stack-php",
  "description": "environment stack for php developers",
  "license": "GLWTPL",
  "authors": [
    {
      "name": "Leonardo Cidral",
      "email": "lcidral@gmail.com"
    }
  ],
  "minimum-stability" : "dev",
  "config" : {
    "bin-dir" : "bin"
  },
  "autoload": {
    "psr-4": {
      "lcidral\\developstack\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2.9"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev" : {
    "phpunit/phpunit": "7.1.1",
    "codeception/codeception": "2.4.x-dev",
    "phpunit/php-code-coverage": "^6.0@dev",
    "captbaritone/mailcatcher-codeception-module": "1.*",
    "codeception/c3": "2.*",
    "mockery/mockery": "dev-master"
  }
}
